{
	"name": "obsidian-boilerplate",
	"version": "1.0.0",
	"description": "A boilerplate template for developing Obsidian plugins with TypeScript, esbuild",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [
		"obsidian",
		"obsidian-plugin",
		"obsidian-md",
		"typescript",
		"boilerplate",
		"template"
	],
	"author": "Adam Smith",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/admjs/obsidian-boilerplate"
	},
	"homepage": "https://github.com/admjs/obsidian-boilerplate",
	"devDependencies": {
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	}
}
